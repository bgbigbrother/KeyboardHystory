{
  "name": "keyboard-history",
  "version": "1.1.3",
  "description": "A browser-based npm package that captures and stores keyboard interaction data including key presses, duration, and timestamps",
  "main": "dist/keyboard-history.umd.js",
  "module": "dist/keyboard-history.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/keyboard-history.es.js",
      "require": "./dist/keyboard-history.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build:clean && vite build && npm run build:types",
    "build:lib": "vite build",
    "build:types": "tsc --project tsconfig.build.json",
    "build:clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "preview": "vite preview",
    "demo": "vite demo",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run lint && npm run test && npm run build"
  },
  "keywords": [
    "keyboard",
    "events",
    "tracking",
    "browser",
    "typescript",
    "analytics",
    "keylogger",
    "input-tracking",
    "user-interaction",
    "web-analytics",
    "event-capture",
    "timing-analysis"
  ],
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/bgbigbrother/KeyboardHystory#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bgbigbrother/KeyboardHystory.git"
  },
  "bugs": {
    "url": "https://github.com/bgbigbrother/KeyboardHystory/issues"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "fast-check": "^3.13.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "terser": "^5.19.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.0"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=16"
  }
}